body.daacs-query {
	font-family: "proxima-nova-n4", "proxima-nova", "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size: 100%;
	line-height: 1.5;
	color: #333333;
	background: #fff;
	min-width: 768px;
}

body.daacs-query header {
	background: #596e71 url(http://drc.daacs.org/images/daacs/bg_masthead.jpg) bottom repeat-x;
	padding: 0;
	margin: 0;
}

body.daacs-query header .container {
	padding: 20px;
}

body.daacs-query #logo {
	float: left;
	margin: 0;
}

body.daacs-query #logo a {
	display: block;
	background: url(http://drc.daacs.org/images/daacs/logo.png) no-repeat;
	width: 195px;
	height: 67px;
	text-indent: 100%;
	white-space: nowrap;
	overflow: hidden;
}

#tagline {
	float: right;
	margin: 2.5rem 0 0;
	color: #ccccaf;
	font-size: 2rem;
	line-height: 1.5rem;
	letter-spacing: 0.01rem;
	font-family: "chaparral-pro-n4","chaparral-pro",serif;
	font-style: normal;
	font-weight: 400;
	text-shadow: 0px 2px 5px rgba(0, 0, 0, 0.5);
}

#navbar {
	background: #ccccaf;
	height: 46px;
}

#navbar a {
	margin-top: 6px;
	float: right;
	margin-right: 150px;
}
.wrap, #inner-header, #nav, .nav, .daacs-query #main, .article-header, #inner-content, .sponsors-list, #inner-footer {
max-width: 980px;
margin: 0 auto;
}

#footer-sponsors:before {
content: "";
position: relative;
top: 5px;
left: 0;
width: 100%;
height: 10px;
-webkit-box-shadow: 1px 1px 7px rgba(0, 0, 0, 0.75);
-moz-box-shadow: 1px 1px 7px rgba(0, 0, 0, 0.75);
box-shadow: 1px 1px 7px rgba(0, 0, 0, 0.75);
z-index: 100;
}

#footer-sponsors {
	padding: 0 30px;
	background: url("http://drc.daacs.org/images/daacs/bg_footer_sponsors.jpg");
	border-bottom: 20px solid #364344;
	border-top: 5px solid #aeb084;
	color: #f4f4eb;
	text-align: center;
}

#footer-sponsors h4 {
	font-family: "chaparral-pro-n4", "chaparral-pro", Georgia, Cambria, Times New Roman, Times, serif;
	font-weight: 200;
	font-size: 1.25rem;
	line-height: 1.3rem;
	letter-spacing: 0.1rem;
	text-transform: uppercase;
	margin: 0;
	position: relative;
	top: -1rem;
	display: inline-block;
	background: #aeb084;
	color: white;
	padding: 0.5em 1em 0.25em;
}

.clearfix:before, #main:before, .article-header:before, #inner-content:before, #inner-header:before, .sponsors-list:before, #inner-footer:before, #nav:before, #homepage-sidebar:before, .clearfloat:before, body.archive .archives:before, body.blog .archives:before, #searchform:before, .sidebar:before, ul.gallery-listing:before, .grid-list ul.gallery-listing li:before, #inner-footer .nav:before, body.single-plantations .images .gallery-details ul:before, body.single-sites .images .gallery-details ul:before, .clearfix:after, #main:after, .article-header:after, #inner-content:after, #inner-header:after, .sponsors-list:after, #inner-footer:after, #nav:after, #homepage-sidebar:after, .clearfloat:after, body.archive .archives:after, body.blog .archives:after, #searchform:after, .sidebar:after, ul.gallery-listing:after, .grid-list ul.gallery-listing li:after, #inner-footer .nav:after, body.single-plantations .images .gallery-details ul:after, body.single-sites .images .gallery-details ul:after {
content: "";
display: table;
}

ul.sponsors-list {
	padding: 0 20px;
	margin: 0 auto 14px;
	list-style-type: none;
}

ul.sponsors-list li:first-child {
	text-align: left;
}

ul.sponsors-list li:first-child {
	border-top: none;
	margin-top: 0;
	padding-top: 0;
}

ul.sponsors-list li:last-child {
	border-right: medium none;
	text-align: right;
}

ul.sponsors-list li {
	margin: 0 0 0.5em 0;
	padding: 0;
	border-top: none;
	border-right: 1px solid #364344;
	float: left;
	line-height: 70px;
	width: 33.333%;
}

.back-to-queries {
	width: 165px;
	margin-left: 25px;
}

.footer {
min-height: 225px;
clear: both;
padding: 0 0 1.5em;
border-top: 4px solid #aeb084;
background: #aeb084 url(http://drc.daacs.org/images/daacs/bg.jpg);
}

#inner-footer {
padding: 3.125rem 1.25rem;
max-width: 980px;
margin: 0 auto;
}

#inner-footer nav {
	float:left;
}

.source-org.copyright {
display: inline;
float: left;
position: relative;
margin-left: 2.76243%;
width: 31.333%;
margin-left: 0;
width: auto;
margin-right: 0;
color: #000;
font-size: 1.35rem;
margin-top: 3px;
}

.copyright {
font-family: "chaparral-pro-n4", "chaparral-pro", Georgia, Cambria, Times New Roman, Times, serif;
font-style: italic;
font-weight: 600;
}

ul.footer-nav {
display: inline;
float: left;
position: relative;
margin-left: 2.76243%;
width: 39.667%;
margin-top: .25em;
margin-top: .25rem;
width: 100%;
margin-left: 0;
margin-right: 0;
}

ul.footer-nav li {
display: inline-block;
}
.nav li {
float: left;
position: relative;
}
ul.footer-nav a, ul.footer-nav a:visited {
color: #f4f4eb;
background: none !important;
padding: 0 10px;
border-left: 1px solid #f4f4eb;
line-height: 1em;
font-size: 1.5rem;
font-family: "proxima-nova-condensed-n4", "proxima-nova-condensed", "Helvetica Neue Condensed", "Arial Narrow", sans-serif;
text-transform: none;
}

ul.footer-nav a:hover {
	color: #000;
}

.social-follow {
text-align: right;
display: inline-block;
float: right;
padding: 0;
}

.social-follow h4 {
font-family: "chaparral-pro-n4", "chaparral-pro", Georgia, Cambria, Times New Roman, Times, serif;
font-style: italic;
font-weight: 600;
}

.social-follow h4 {
float: left;
margin: 0 0.5em 2em 0;
color: #333333;
}

.licensing {
display: inline;
float: left;
position: relative;
margin-left: 2.76243%;
width: 39.667%;
margin-left: 0;
font-size: 1.25rem;
line-height: 1.6rem;
vertical-align: middle;
clear: both;
color: #333333;
}

.licensing a {
font-weight: 600;
color: #333333;
}

.licensing img {
display: block;
float: left;
margin-right: 10px;
margin-right: 0.625rem;
}