<div class="page-header page-header-01">
	<div class="frame">
		<div class="clearfix">
			<%= link_to 'Back to List', ceramics_path, :class => 'back-link'%>
		</div>
		<div class="paging">
			<!-- <a href="#" class="next">Next</a> -->
			<% if @in_context.index(@ceramic) != nil %>
			<% if @in_context.index(@ceramic) < (@in_context.size-1) %>
				<%= link_to 'Next', edit_ceramic_path(@in_context[@in_context.index(@ceramic)+1].id), {:class => 'next'}%>
			<% end %>
			<span class="caption"><%= @in_context.index(@ceramic)+1 %> of <%= @in_context.size %></span>
			<% if @in_context.index(@ceramic) > 0%>
				<%= link_to 'Previous', edit_ceramic_path(@in_context[@in_context.index(@ceramic)-1].id), {:class => 'prev'}%>
			<% end %>
			<% end %>
			<!-- <a href="#" class="prev">Previous</a> -->
		</div><!-- paging -->
	</div><!-- frame -->
	<h1>Ceramic Artifact</h1>
	<% if current_user.is_editor?  %>
	<a href="#" data-toggle="modal" data-target="#add-new" class="add-new new-ceramic-type">Add New</a>
	<% end %>
</div><!-- page-header -->
<%= render 'layouts/messages', errors: @ceramic.errors %>
<div class="clearfix">
	<%= form_for(@ceramic) do |f|%>
		<%= f.hidden_field(:GenerateContextArtifactID)%>
		<input type="hidden" name="active_tag" id="active_tag">
		<fieldset>
			<aside id="aside">
				<h2 class="visible-md visible-lg">Record Details</h2>
				<% if @ceramic.context_sample == nil or current_user.edit_projects.include?(@ceramic.context_sample.project) or current_user.is_admin?  %>
				<div class="box">
					<% if @ceramic.context_sample != nil %>
						<%= link_to 'Add Artifact to Context', {:action => 'new', :context_sample => @ceramic.context_sample.ContextSampleID}, {:class => 'add-new'}%>
					<% end %>
					<!-- <a href="#" data-toggle="modal" data-target="#add-new" class="add-new">Add New record</a> -->
				</div><!-- box -->
				<div class="box">
					<button id="button-record-save" class="btn btn-success btn-save ie-fix"><span>Save</span></button>
					<%= image_tag('fancybox_loading.gif', :style => 'display:none', :id => 'loading-icon') %>
					<div class="area clearfix">
						<%= link_to('Duplicate', {:action => 'duplicate', :id => params[:id]}, {:class=>"btn btn-info btn-duplicate ie-fix"})%>
						<%# if current_user.user_level >= 3 or current_user.user_level == 2  %>
						<% if current_user.deletable_projects.include?(@ceramic.project) or current_user.is_admin? %>
							<a href="#" data-toggle="modal" data-target="#delete-alert-modal" class="delete">Delete</a>
						<% end %>
					</div>
				</div><!-- box -->
				<% end %>
				<div class="details-bar">
					<a href="#" data-toggle="dropdown" class="open-close"></a>
					<div class="drop dropdown-menu" role="menu">
						<div class="info-box">
							<dl>
								<dt class="visible-md visible-lg">Artifact ID:</dt><dd class="visible-md visible-lg"><%= @ceramic.ArtifactID != nil ? @ceramic.ArtifactID : ' - ' %></dd>
								<dt>Project:</dt><dd><%= (@ceramic.context_sample != nil and @ceramic.context_sample.project != nil) ? @ceramic.context_sample.project.project_name.ProjectName : ' - ' %></dd>
								<dt>Context Sample:</dt><dd><%= @ceramic.context_sample != nil ? @ceramic.context_sample.ContextSampleID.gsub(';','') : ' - ' %></dd>
							</dl>
							<dl>
								<dt>ware:</dt><dd><%= @ceramic.ceramic_ware != nil ? @ceramic.ceramic_ware.Ware : ' - ' %></dd>
								<dt>form:</dt><dd><%= @ceramic.ceramic_form != nil ? @ceramic.ceramic_form.CeramicForm : ' - ' %></dd>
								<dt>manu tech:</dt><dd><%= @ceramic.ceramic_manu_tech != nil ? @ceramic.ceramic_manu_tech.CeramicManuTech : ' - ' %></dd>
								<dt>completeness:</dt><dd><%= @ceramic.ceramic_completeness != nil ? @ceramic.ceramic_completeness.CeramicCompleteness : ' - ' %></dd>
								<dt>decoration:</dt><dd><%= @ceramic.decoration_yes_no.YesNo if @ceramic.decoration_yes_no != nil %></dd>
							</dl>
						</div><!-- info-box -->
						<div class="aside-accordion" id="aside-accordion">
							<div class="panel">
								<h2><a class="collapsed" data-toggle="collapse" data-parent="#aside-accordion" href="#collapse-01">all ceramics in context</a></h2>
								<div id="collapse-01" class="panel-collapse collapse">
									<div class="frame">
										<div class="artifacts scrollable-area">
											<ul>
												<% @in_context.each do |ceramic| %>
													<!--  check for saving!!! -->
													<li <%= @ceramic.ArtifactID == ceramic.ArtifactID ? 'class=current' : '' %>><%= @ceramic.ArtifactID != ceramic.ArtifactID ? link_to(ceramic.ArtifactID, edit_ceramic_path(ceramic)) : ceramic.ArtifactID %></li>
												<% end %>
											</ul>
										</div><!-- artifacts -->
									</div>
								</div>
							</div>
							<div class="panel">
								<h2 class="visible-md visible-lg"><a class="collapsed" data-toggle="collapse" data-parent="#aside-accordion" href="#collapse-02">Cataloger Details</a></h2>
								<div id="collapse-02" class="panel-collapse collapse">
									<div class="details-box">
										<dl>
											<% user = User.where('editor_id = ?', @ceramic.EditorIDN).first %>
											<dt>Added By:</dt><dd><%= user != nil ? "#{(user.first_name != nil and user.first_name != '') ? user.first_name+' '+user.last_name : user.userid}" : ' - '%></dd>
											<dt>Date Added:</dt><dd><%=h @ceramic.DateAdded !=nil ? @ceramic.DateAdded.strftime('%m/%d/%Y %H:%M:%S') : " - "%></dd>
										</dl>
										<dl>
											<% user = User.where('editor_id = ?', @ceramic.EditorID).first %>
											<dt>Changed By:</dt><dd><%= user != nil ? "#{(user.first_name != nil and user.first_name != '') ? user.first_name+' '+user.last_name : user.userid}" : ' - '%></dd>
											<dt>Date Changed:</dt><dd><%=h @ceramic.DateChanged !=nil ? @ceramic.DateChanged.strftime('%m/%d/%Y %H:%M:%S') : " - "%></dd>
										</dl>
										<div class="clearfix">
											<a href="#" class="link">Change Log</a>
										</div>
									</div><!-- details-box -->
								</div>
							</div>
							<div class="panel">
								<% if @ceramic.artifact_images.size > 0 %>
								<h2 class="visible-md visible-lg"><a class="collapsed" data-toggle="collapse" data-parent="#aside-accordion" href="#collapse-03">Images</a></h2>
								<div id="collapse-03" class="panel-collapse collapse">
									<script>
										function showFancybox(){		
										if ($('.fancybox').exists()){
											$('.fancybox').fancybox({
												href : $('.item.active').children()[0].src,
												prevEffect : 'fade',
												nextEffect : 'fade',
											});
										}
										}
										
									</script>
									<div class="slideshow slide" id="slideshow-01" data-ride="carousel" data-interval="false">
										<div class="carousel-inner">
											<% first = true %>
											<% @ceramic.artifact_images.each do |artifact_image| %>
												<div class="item <%= first == true ? 'active' : '' %>">
													<%= image_tag(artifact_image.image.ImageFileLocation) if artifact_image.image != nil%>
												</div>
												<% first = false %>
											<% end %>
										</div>
										<a class="loupe fancybox" onclick="showFancybox();return false;" href="#">loupe</a>
										<a class="prev-btn" href="#slideshow-01" data-slide="prev">&#60;</a>
										<a class="next-btn" href="#slideshow-01" data-slide="next">&#62;</a>
									</div><!-- slideshow -->
								</div>
								<% end %>
							</div>
						</div><!-- accordion -->
					</div>
				</div><!-- details-bar -->
			</aside><!-- aside -->
