<h1>Listing button_join_methods</h1>

<table>
  <tr>
    <th>Button join method</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @button_join_methods.each do |button_join_method| %>
  <tr>
    <td><%= button_join_method.button_join_method %></td>
    <td><%= link_to 'Show', button_join_method %></td>
    <td><%= link_to 'Edit', edit_button_join_method_path(button_join_method) %></td>
