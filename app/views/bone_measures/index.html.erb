<h1>Listing bone_measures</h1>

<table>
  <tr>
    <th>Artifact</th>
    <th>Measuring description</th>
    <th>Measurement</th>
    <th>Accurate</th>
    <th>Generate context artifact</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bone_measures.each do |bone_measure| %>
  <tr>
    <td><%= bone_measure.artifact_id %></td>
    <td><%= bone_measure.measuring_description_id %></td>
    <td><%= bone_measure.measurement %></td>
    <td><%= bone_measure.accurate %></td>
    <td><%= bone_measure.generate_context_artifact_id %></td>
    <td><%= link_to 'Show', bone_measure %></td>
    <td><%= link_to 'Edit', edit_bone_measure_path(bone_measure) %></td>
    <td><%= link_to 'Destroy', bone_measure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Bone measure', new_bone_measure_path %>
