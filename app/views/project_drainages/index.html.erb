<h1>Listing project_drainages</h1>

<table>
  <tr>
    <th>Drainage</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @project_drainages.each do |project_drainage| %>
  <tr>
    <td><%= project_drainage.drainage %></td>
    <td><%= link_to 'Show', project_drainage %></td>
    <td><%= link_to 'Edit', edit_project_drainage_path(project_drainage) %></td>
    <td><%= link_to 'Destroy', project_drainage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

