<h1>Listing bead_materials</h1>

<table>
  <tr>
    <th>Bead material</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bead_materials.each do |bead_material| %>
  <tr>
    <td><%= bead_material.bead_material %></td>
    <td><%= link_to 'Show', bead_material %></td>
    <td><%= link_to 'Edit', edit_bead_material_path(bead_material) %></td>
    <td><%= link_to 'Destroy', bead_material, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

