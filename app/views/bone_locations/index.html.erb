<h1>Listing bone_locations</h1>

<table>
  <tr>
    <th>Bone location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bone_locations.each do |bone_location| %>
  <tr>
    <td><%= bone_location.bone_location %></td>
    <td><%= link_to 'Show', bone_location %></td>
    <td><%= link_to 'Edit', edit_bone_location_path(bone_location) %></td>
    <td><%= link_to 'Destroy', bone_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Bone location', new_bone_location_path %>
