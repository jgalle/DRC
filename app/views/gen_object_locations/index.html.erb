<h1>Listing gen_object_locations</h1>

<table>
  <tr>
    <th>Object</th>
    <th>Loaned to</th>
    <th>Institution contact</th>
    <th>Exhibit name</th>
    <th>Date loaned</th>
    <th>Date returned</th>
    <th>Artifact handler</th>
    <th>Notes</th>
    <th>Loan length</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @gen_object_locations.each do |gen_object_location| %>
  <tr>
    <td><%= gen_object_location.object_id %></td>
    <td><%= gen_object_location.loaned_to %></td>
    <td><%= gen_object_location.institution_contact %></td>
    <td><%= gen_object_location.exhibit_name %></td>
    <td><%= gen_object_location.date_loaned %></td>
    <td><%= gen_object_location.date_returned %></td>
    <td><%= gen_object_location.artifact_handler %></td>
    <td><%= gen_object_location.notes %></td>
    <td><%= gen_object_location.loan_length %></td>
    <td><%= link_to 'Show', gen_object_location %></td>
    <td><%= link_to 'Edit', edit_gen_object_location_path(gen_object_location) %></td>
    <td><%= link_to 'Destroy', gen_object_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

